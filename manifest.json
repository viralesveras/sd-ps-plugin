{
  "id": "8a9aeca1",
  "name": "sd-ps-plugin",
  "version": "0.0.2",
  "main": "index.html",
  "data": {
    "addToPluginMenu": true,
    "loadEvent": "startup"
  },
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0"
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "main",
      "minimumSize": {
        "width": 280,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 280,
        "height": 800
      },
      "preferredFloatingSize": {
        "width": 280,
        "height": 800
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/dark.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/light.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ],
      "label": {
        "default": "Diffusion"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ]
    }
  ],
  "requiredPermissions": {
    "network": {
      "domains": "all"
    },
    "clipboard": "readAndWrite",
    "localFileSystem": "fullAccess",
    "launchProcess": "request" 
  }
}